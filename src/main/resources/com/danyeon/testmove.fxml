<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.LineTo?>
<?import javafx.scene.shape.MoveTo?>
<?import javafx.scene.shape.Path?>
<?import javafx.scene.shape.Rectangle?>

<Pane fx:id="rootNode" prefHeight="2000.0" prefWidth="2000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.danyeon.testMoveController">
   <children>
      <Rectangle fx:id="car" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" focusTraversable="true" height="80.0" stroke="BLACK" strokeType="INSIDE" width="40.0" x="200.0" y="160.0" />
      <Pane fx:id="map" prefHeight="2000.0" prefWidth="2000.0">
         <children>
            <Path fx:id="path" fill="DODGERBLUE" stroke="BLACK">
                <elements>
                    <MoveTo />
                    <LineTo x="2000.0" y="300.0" />
                </elements>
            </Path>
         </children>
      </Pane>
   </children>
</Pane>
